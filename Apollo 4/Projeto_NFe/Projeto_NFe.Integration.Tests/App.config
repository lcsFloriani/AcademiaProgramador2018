<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- DataProvider. Somente usado em ADO.NET. As opções são: System.Data.OleDb, System.Data.SqlClient, or System.Data.OracleClient. -->
    <add key="DataProvider" value="System.Data.SqlClient"/>
    <!-- ConnectionName. O nome da string de conexão.-->
    <add key="ConnectionStringName" value="DBProjetoNFe"/>
  </appSettings>
  <connectionStrings>
    <add name="DBProjetoNFe" connectionString="Data Source=(localdb)\mssqllocaldb;Initial Catalog=DBProjetoNFe; Integrated Security=True; MultipleActiveResultSets=True;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup></configuration>
